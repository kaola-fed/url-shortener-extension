{
  "manifest_version": 2,
  "name": "URL Shortener",
  "version": "1.0.2",
  "browser_action": {
    "name": "Shorten URL!"
  },
  "icons": {
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}